{
    "name": "Lectio Master",
    "description": "Lectio men bedre",
    "manifest_version": 2,
    "version": "0.5.0",
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://*.lectio.dk/*"],
            "js": ["js/opgaver.js", "js/skema.js", "js/karakter.js", "js/forside.js", "js/skolevalg.js", "/js/login.js"],
            "css": ["css/main.css"],
            "web_accessible_resources": ["images/lectio_logo.png"],
            "all_frames": true
        }
    ],
    "permissions": ["storage"]
}